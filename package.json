{
    "name": "sales-coach-ai",
    "version": "1.0.0",
    "description": "A full-stack sales coach AI application",
    "main": "index.js",
    "scripts": {
        "start": "concurrently \"npm run start:client\" \"npm run start:server\"",
        "start:client": "cd client && npm start",
        "start:server": "cd server && node server.js",
        "install": "npm install && cd client && npm install && cd ../server && npm install",
        "build": "cd client && npm run build",
        "test": "cd client && npm test && cd ../server && npm test"
    },
    "keywords": [
        "sales",
        "AI",
        "coach",
        "react",
        "node"
    ],
    "author": "Your Name",
    "license": "ISC",
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "autoprefixer": "^10.4.19",
        "concurrently": "^6.5.1"
    },
    "dependencies": {
        "axios": "^1.7.2",
        "firebase": "^10.12.4",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.25.1"
    }
}
